<%= bootstrap_form_for(@node, remote: true) do |f| %>
	<div class="field">
		<%= render "node_fields", f: f, disabled: check_editor_privilege.! %>
		<%= link_to 'Back', :back, class: 'btn btn-default' %>
		<% if check_editor_privilege %>
      	<%= f.submit("Update", class: 'btn btn-primary') %>
		<% end %>
  </div>
<% end %>